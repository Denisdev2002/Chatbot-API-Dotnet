name: dockercompose15594904852630344161
services:
  application:
    build:
      context: C:\Users\Interfocus\Documents\ChatBot-Api-Dotnet
      dockerfile: Dockerfile
    container_name: redisApplication
    depends_on:
      redis:
        condition: service_started
        required: true
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTP_PORTS: "8080"
      ASPNETCORE_HTTPS_PORTS: "8081"
      Redis:ConnectionString: redis:6379
    image: redis:latest
    networks:
      app-network: null
    ports:
      - mode: ingress
        target: 8080
        published: "5091"
        protocol: tcp
      - mode: ingress
        target: 8080
        protocol: tcp
      - mode: ingress
        target: 8081
        protocol: tcp
    volumes:
      - type: volume
        source: app-data
        target: /app/volumes
        volume: {}
      - type: bind
        source: C:\Users\Interfocus\AppData\Roaming/Microsoft/UserSecrets
        target: /home/app/.microsoft/usersecrets
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\Interfocus\AppData\Roaming/ASP.NET/Https
        target: /home/app/.aspnet/https
        read_only: true
        bind:
          create_host_path: true
  redis:
    container_name: redis
    image: redis:latest
    networks:
      app-network: null
    ports:
      - mode: ingress
        target: 6379
        published: "6379"
        protocol: tcp
networks:
  app-network:
    name: dockercompose15594904852630344161_app-network
    driver: bridge
volumes:
  app-data:
    name: dockercompose15594904852630344161_app-data
    driver: local